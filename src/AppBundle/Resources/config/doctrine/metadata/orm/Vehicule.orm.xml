<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Vehicule" table="vehicule">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="description" type="string" column="description" length="254" nullable="true"/>
    <field name="immatriculation" type="string" column="immatriculation" length="254" nullable="true"/>
    <field name="dateachat" type="datetime" column="dateAchat" nullable="true"/>
    <field name="valeur" type="decimal" column="valeur" precision="8" scale="0" nullable="true"/>
  </entity>
</doctrine-mapping>
