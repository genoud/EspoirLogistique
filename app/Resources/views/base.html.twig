<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Homepage{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" type="text/css" href="{{asset('lib/startbootstrap-sb-admin-1.0.3/css/bootstrap.min.css')}}">
            <link rel="stylesheet" type="text/css" href="{{asset('lib/startbootstrap-sb-admin-1.0.3/css/sb-admin.css')}}">
            <link rel="stylesheet" type="text/css" href="{{asset('lib/startbootstrap-sb-admin-1.0.3/css/plugins/morris.css')}}">
            <link rel="stylesheet" type="text/css" href="{{asset('lib/startbootstrap-sb-admin-1.0.3/font-awesome/css/font-awesome.min.css')}}">
            <link rel="stylesheet" type="text/css" href="{{asset('ressources/css/style.css')}}">
            {#            <link rel="stylesheet" type="text/css" href="{{asset('lib/bootstrap-3.3.5-dist/css/bootstrap-theme.min.css')}}">#}
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}

            <div id="wrapper">

                <!-- Navigation -->
                {% block navigation %}
                    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
                        <!-- Brand and toggle get grouped for better mobile display -->
                        {% block headercmp %}
                            {{include('::Components/HeaderCmp.html.twig')}}
                        {% endblock %}

                        <!-- Top Menu Items -->
                        {% block topnavigation %}
                            {{include('::Components/RightTopNav.html.twig')}}
                        {% endblock %}

                        <!-- Sidebar Menu Items - These collapse to the responsive navigation menu on small screens -->
                        {% block sidenavigation %}
                            {{include('::Components/SidebarMenu.html.twig')}}
                        {% endblock %}
                        <!-- /.navbar-collapse -->
                    </nav>
                {% endblock %}


                {% block maincontent %}
                    <div id="page-wrapper">


                        {% block pagewrapper %}
                            {{include('::Components/Dashboard.html.twig')}}
                        {% endblock %}

                    </div>
                {% endblock %}

                
            </div>

        {% endblock %}
        {% block javascripts %}
            <!-- jQuery -->
            <script src="{{asset('lib/startbootstrap-sb-admin-1.0.3/js/jquery.js')}}"></script>
            

            <!-- Bootstrap Core JavaScript -->
            <script src="{{asset('lib/startbootstrap-sb-admin-1.0.3/js/bootstrap.min.js')}}"></script>

            <!-- Morris Charts JavaScript -->
            <script src="{{asset('lib/startbootstrap-sb-admin-1.0.3/js/plugins/morris/raphael.min.js')}}"></script>
            <script src="{{asset('lib/startbootstrap-sb-admin-1.0.3/js/plugins/morris/morris.min.js')}}"></script>
            <script src="{{asset('lib/startbootstrap-sb-admin-1.0.3/js/plugins/morris/morris-data.js')}}"></script>
            {#<script src="js/plugins/morris/raphael.min.js"></script>
            <script src="js/plugins/morris/morris.min.js"></script>
            <script src="js/plugins/morris/morris-data.js"></script>#}
        {% endblock %}
    </body>
</html>
